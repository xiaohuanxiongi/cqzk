<view class="head" style="height: {{ barHeight }}px">
  <view class="head__title" style="height: {{ navHead }}px;padding-top: {{ statusBarHeight }}px">
    <view class="avatar">
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <open-data type="userNickName"></open-data>
  </view>
</view>
<scroll-view class="container" scroll-y style="padding-top: {{ barHeight }}px">
  <view class="warp">
    <text bindtap="jump" data-key="login">授权登录</text>
    <text bindtap="jump" data-key="years">历年成绩查询</text>
    <text bindtap="jump" data-key="seat">座位号查询</text>
  </view>
</scroll-view>
