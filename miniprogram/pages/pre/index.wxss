.warp {
  text-align: center;
}
.warp .info-item {
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  position: relative;
  font-size: 26rpx;
}
.warp .info-item:before {
  content: '';
  height: 2rpx;
  width: 100%;
  background-color: #f2f2f2;
  position: absolute;
  bottom: 0;
  left: 30rpx;
}
.warp .info-item .label {
  color: rgba(0, 0, 0, 0.7);
}
.warp .list {
  display: flex;
  flex-direction: column;
}
.warp .list-title {
  height: 70rpx;
  line-height: 70rpx;
  background-color: #f2f2f2;
  text-align: left;
  font-size: 26rpx;
  padding: 0 30rpx;
  color: #8f8f8f;
}
.warp .list-title .sub {
  font-size: 20rpx;
}
.warp .list-cell {
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  position: relative;
  font-size: 26rpx;
}
.warp .list-cell:before {
  content: '';
  height: 2rpx;
  width: 100%;
  background-color: #f2f2f2;
  position: absolute;
  bottom: 0;
  left: 30rpx;
}
.warp .list-cell .success {
  color: rgba(7, 193, 96, 0.9);
}
.warp .tip {
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
